{
  "kind": "collectionType",
  "collectionName": "event_categories",
  "info": {
    "singularName": "event-category",
    "pluralName": "event-categories",
    "displayName": "Event Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
